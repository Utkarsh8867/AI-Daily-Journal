{
    "name": "ai-daily-journal",
    "version": "1.0.0",
    "description": "AI-powered daily journaling assistant with beautiful UI and intelligent insights",
    "main": "index.js",
    "scripts": {
        "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
        "dev:frontend": "cd frontend && npm run dev",
        "dev:backend": "cd backend && python api_server.py",
        "build": "cd frontend && npm run build",
        "start": "cd frontend && npm start",
        "install:all": "npm run install:frontend && npm run install:backend",
        "install:frontend": "cd frontend && npm install",
        "install:backend": "cd backend && pip install -r requirements.txt",
        "deploy": "cd frontend && vercel --prod"
    },
    "keywords": [
        "ai",
        "journal",
        "nextjs",
        "fastapi",
        "maestro",
        "emotion-detection",
        "mood-tracking",
        "wellness"
    ],
    "author": "Utkarsh Kadu",
    "license": "MIT",
    "devDependencies": {
        "concurrently": "^8.2.2"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Utkarsh8867/AI-Daily-Journal.git"
    },
    "engines": {
        "node": ">=16.0.0",
        "python": ">=3.8.0"
    }
}
