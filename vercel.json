{
    "version": 2,
    "name": "ai-daily-journal",
    "builds": [
        {
            "src": "frontend/package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/backend/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/frontend/$1"
        }
    ],
    "functions": {
        "backend/api/journal/process.py": {
            "runtime": "@vercel/python"
        },
        "backend/api/journal/entries.py": {
            "runtime": "@vercel/python"
        },
        "backend/api/journal/analytics.py": {
            "runtime": "@vercel/python"
        }
    }
}